# Base URL for backend API endpoints
VITE_API_BASE_URL=http://localhost:3000/api/v1
# API request timeout in milliseconds
VITE_API_TIMEOUT=30000

# Azure AD B2C Authentication Configuration
# Client ID from Azure AD B2C application registration
VITE_AZURE_CLIENT_ID=your_azure_client_id
# Azure AD B2C authority URL with policy configuration
VITE_AZURE_AUTHORITY=https://your_tenant.b2clogin.com/your_tenant.onmicrosoft.com/your_policy
# Azure AD B2C known authorities domain
VITE_AZURE_KNOWN_AUTHORITIES=your_tenant.b2clogin.com

# Medplum FHIR Server Configuration
# Base URL for Medplum FHIR R4 server
VITE_MEDPLUM_SERVER_URL=https://api.medplum.com/fhir/R4
# Client ID for Medplum FHIR server authentication
VITE_MEDPLUM_CLIENT_ID=your_medplum_client_id

# Node Environment
# Must be either 'development' or 'production'
VITE_NODE_ENV=development